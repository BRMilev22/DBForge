{
  "name": "dbforge-framework",
  "version": "0.3.0",
  "description": "DBForge client framework with AES-256 encryption, SQL protection, TypeScript support and dual public/authorized API modes.",
  "type": "module",
  "main": "src/client.js",
  "types": "src/client.d.ts",
  "exports": {
    ".": {
      "import": "./src/client.js",
      "types": "./src/client.d.ts"
    },
    "./crypto": {
      "import": "./src/crypto.js",
      "types": "./src/crypto.d.ts"
    },
    "./sqlSafe": {
      "import": "./src/sqlSafe.js",
      "types": "./src/sqlSafe.d.ts"
    },
    "./bundle": {
      "import": "./dbforge-framework.ts",
      "types": "./dbforge-framework.ts"
    }
  },
  "dependencies": {
    "crypto": "^1.0.1"
  }
}
