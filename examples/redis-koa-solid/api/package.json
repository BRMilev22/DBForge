{
  "name": "dbforge-redis-koa",
  "version": "0.1.0",
  "description": "Koa API that showcases DBForge-managed Redis via the DbForge framework.",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "start": "NODE_ENV=production tsx src/server.ts",
    "db:bootstrap": "tsx scripts/bootstrap.ts",
    "build": "tsc -p tsconfig.json && node dist/server.js"
  },
  "keywords": [
    "dbforge",
    "redis",
    "koa"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@koa/cors": "^5.0.0",
    "@koa/router": "^14.0.0",
    "cors": "^2.8.5",
    "dbforge-framework": "^0.1.0",
    "dotenv": "^17.2.3",
    "koa": "^3.1.1",
    "koa-bodyparser": "^4.4.1",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  }
}
