{
  "name": "dbforge-mongo-express",
  "version": "0.1.0",
  "description": "Express API that showcases DBForge-managed MongoDB via the DbForge framework.",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "start": "NODE_ENV=production tsx src/server.ts",
    "db:bootstrap": "tsx scripts/bootstrap.ts",
    "build": "tsc -p tsconfig.json && node dist/server.js"
  },
  "keywords": [
    "dbforge",
    "mongodb",
    "express"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.5",
    "dbforge-framework": "^0.1.0",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "mongodb": "^7.0.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/node": "^24.10.1",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  }
}
